{
  "manifest_version": 3,
  "name": "Temp Mail",
  "version": "1.0",
  "description": "Temporary Email Chrome Extension",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "permissions": [
    "storage",
    "activeTab"
  ]
}
